<?xml version='1.0' encoding='utf-8'?>
<widget id="app.vercel.pokesnap" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Pokesnap</name>
  <description>An app for Pokemon fans</description>
  <author email="johnnyliao06@gmail.com" href="https://github.com/Jawny">Johnny Liao</author>
  <content src="index.html" />
  <access origin="*" />
  <icon src="public/assets/pokeball.png" />
  <platform name="android">
    <icon src="public/assets/pokeball.png" density="mdpi" />
    <icon src="public/assets/pokeball.png" density="hdpi" />
    <icon src="public/assets/pokeball.png" density="xhdpi" />
    <icon src="public/assets/pokeball.png" density="xxhdpi" />
    <icon src="public/assets/pokeball.png" density="xxxhdpi" />
  </platform>
  <platform name="ios">
    <icon src="public/assets/pokeball.png" width="57" height="57" />
    <icon src="public/assets/pokeball.png" width="72" height="72" />
    <icon src="public/assets/pokeball.png" width="76" height="76" />
    <icon src="public/assets/pokeball.png" width="114" height="114" />
    <icon src="public/assets/pokeball.png" width="120" height="120" />
    <icon src="public/assets/pokeball.png" width="144" height="144" />
    <icon src="public/assets/pokeball.png" width="152" height="152" />
    <icon src="public/assets/pokeball.png" width="167" height="167" />
    <icon src="public/assets/pokeball.png" width="180" height="180" />
  </platform>
</widget>
